          , TW_COMENT_FILE
        ) VALUES (
            #{twComentNo}
          , #{topWorkNo}
          , #{memberId}
          , SYSDATE
          , #{twComentContent}
          , 0
        )
   </insert>
   
   <update id="topWorkComentDelete" parameterType="topWorkComentVO">
        UPDATE TOPWORK_COMENT
           SET TW_COMENT_STATE = '1'
