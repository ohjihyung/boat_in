   </select>
   
   <update id="moveState" parameterType="topWorkVO">
        UPDATE TOPWORK
           SET TOPWORK_STATE = #{topWorkState}
         WHERE TOPWORK_NO = #{topWorkNo} 
   </update>
   
   <select id="getCategoryName" parameterType="string" resultType="string">
		SELECT TW_CATEGORY_NAME FROM TOPWORK_CATEGORY
		WHERE TW_CATEGORY_NO = #{categoryNo}
   </select>
   
   <select id="logList" parameterType="topWorkVO" resultType="projectLogVO">
        SELECT *
          FROM PROJECT_LOG
         WHERE TOPWORK_NO = #{topWorkNo}
           AND PRO_LOG_STATE = '0'
           AND SUBWORK_NO = 'F'
         ORDER BY PRO_LOG_NO ASC
   </select>
   
   <select id="logListChart" parameterType="topWorkVO" resultType="projectLogVO">
        SELECT *
          FROM PROJECT_LOG
         WHERE TOPWORK_NO = #{topWorkNo}
           AND PRO_LOG_STATE = '0'
           AND SUBWORK_NO = 'F'
           AND PRO_LOG_CONTENT LIKE '%진행률%'
         ORDER BY PRO_LOG_NO ASC
   </select>
   
   
   <select id="getTopWorkTitle" parameterType="string" resultType="string">
   		SELECT TOPWORK_TITLE 
   		  FROM TOPWORK
   		 WHERE TOPWORK_NO = #{topWorkNo}
   </select>
   <!-- 
   <select id="selectMemberList" parameterType="topWorkVO" resultType="topWorkRelationVO">
        SELECT A.*
		     , B.MEMBER_NAME
		     , B.MEMBER_PIC
		     , B.MEMBER_STATE
		  FROM TOPWORK_RELATION A
		  LEFT JOIN MEMBER B
		    ON A.MEMBER_ID = B.MEMBER_ID
		 WHERE A.TOPWORK_NO = #{topWorkNo}
   </select>
    -->
   <select id="selectMemberList" parameterType="topWorkVO" resultType="topWorkRelationVO">
        SELECT E.TOPWORK_R_NO
		     , E.TOPWORK_NO
		     , E.MEMBER_ID
		     , E.MEMBER_NAME
		     , E.MEMBER_PIC
		     , E.MEMBER_STATE
		     , F.PRO_J_S_NO
		     , F.PRO_AUTH
		     , F.PRO_JOINSTATE
		  FROM (
			    SELECT A.TOPWORK_R_NO
			         , A.TOPWORK_NO
			         , A.MEMBER_ID
			         , B.MEMBER_NAME
			         , B.MEMBER_PIC
			         , B.MEMBER_STATE
			      FROM TOPWORK_RELATION A
			     INNER JOIN MEMBER B
			        ON A.MEMBER_ID = B.MEMBER_ID
			     WHERE A.TOPWORK_NO = #{topWorkNo}
			    ) E
     LEFT JOIN (
			    SELECT C.*
			      FROM PROJECT_JOIN_STATE C
			      LEFT JOIN TOPWORK D
			        ON C.PRO_NO = D.PRO_NO
			     WHERE D.TOPWORK_NO = #{topWorkNo}
			    ) F
		    ON E.MEMBER_ID = F.MEMBER_ID
   </select>
   
   <update id="topWorkUpdate_K" parameterType="topWorkVO">
