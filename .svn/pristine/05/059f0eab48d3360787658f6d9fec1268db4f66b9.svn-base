package kr.or.ddit.vo;

public class AlramVO {
    private String alramNo;
    private String alramId;
    private String alramContent;
    private String alramDate;
    private String alramChk;
    public String getAlramNo() {
        return alramNo;
    }
    public void setAlramNo(String alramNo) {
        this.alramNo = alramNo;
    }
    public String getAlramId() {
        return alramId;
    }
    public void setAlramId(String alramId) {
        this.alramId = alramId;
    }
    public String getAlramContent() {
        return alramContent;
    }
    public void setAlramContent(String alramContent) {
        this.alramContent = alramContent;
    }
    public String getAlramDate() {
        return alramDate;
    }
    public void setAlramDate(String alramDate) {
        this.alramDate = alramDate;
    }
    public String getAlramChk() {
        return alramChk;
    }
    public void setAlramChk(String alramChk) {
        this.alramChk = alramChk;
    }
    
    
    
}
