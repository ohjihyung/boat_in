  function drag(target) {
    target.forEach((container) => {
      //인자로 자식 div들을 받음
      //console.log("드래그왜안돼 : ", container);
      new Sortable(container, {
        group: {
          name: "shared", //이게 같은 Sortable 객체들끼리 이동 가능
        },
        swapThreshold: 1,
